:"components/Item.js","original":{"line":79,"column":6}},{"generated":{"line":121,"column":14},"source":"components/Item.js","original":{"line":51,"column":4}},{"generated":{"line":121,"column":15},"source":"components/Item.js","original":{"line":50,"column":2}},{"generated":{"line":122,"column":0},"source":"components/Item.js","original":{"line":84,"column":1}},{"name":"Item","generated":{"line":124,"column":15},"source":"components/Item.js","original":{"line":86,"column":15}}],"sources":{"components/Item.js":"import React, { useEffect, useState } from 'react';\nimport { Route, NavLink, useHistory } from 'react-router-dom';\nimport axios from 'axios';\n\nimport ItemDescription from './ItemDescription';\nimport ItemShipping from './ItemShipping';\n\nfunction Item(props) {\n  console.log('props in Item: ', props);\n  const [item, setItem] = useState({});\n  const { id } = props.match.params;\n\n  const { push } = useHistory();\n\n  useEffect(()=>{\n    axios.get(`http://localhost:3333/items/${id}`)\n      .then(res=>{\n        setItem(res.data);\n      });\n  }, []);\n\n  if (!item) {\n    return <h2>Loading item data...</h2>;\n  }\n\n  const handelEdit = () => {\n    //1. Capture a click of the edit button.\n    //2. redirect the user to the edit form.\n    push(`/item-update/${id}`);\n    \n  }\n\n  //To Delete:\n  //1. Capture a click.\n  //2. Send our axios call to delete current item (id)\n  //3. Redirect user to item list page.\n  //4. Update local state\n  const handelDelete = () => {\n    axios.delete(`http://localhost:3333/items/${id}`)\n    .then (resp => {\n      // console.log('resp: ', resp);\n      props.setItems(resp.data);\n      push(\"/item-list/\");\n    })\n    .catch (err => {\n      console.log(err);\n    })\n  }\n\n  return (\n    <div className=\"item-wrapper\">\n      <div className=\"item-header\">\n        <div className=\"image-wrapper\">\n          <img src={item.imageUrl} alt={item.name} />\n        </div>\n        <div className=\"item-title-wrapper\">\n          <h2>{item.name}</h2>\n          <h4>${item.price}</h4>\n        </div>\n      </div>\n      <nav className=\"item-sub-nav\">\n        <NavLink exact to={`/item-list/${item.id}`}>\n          the story\n        </NavLink>\n        <NavLink to={`/item-list/${item.id}/shipping`}>shipping</NavLink>\n      </nav>\n      <Route\n        exact\n        path=\"/item-list/:id\"\n        render={props => <ItemDescription {...props} item={item} />}\n      />\n      <Route\n        path=\"/item-list/:id/shipping\"\n        render={props => <ItemShipping {...props} item={item} />}\n      />\n      <button onClick = {handelEdit} className=\"md-button\">\n        Edit\n      </button>\n      <button onClick = {handelDelete} className=\"md-button\">\n        Delete\n      </button>\n    </div>\n  );\n}\n\nexport default Item;\n"},"lineCount":null}},"error":null,"hash":"eb1d500a7d394e5393f209445717af93","cacheData":{"env":{}}}